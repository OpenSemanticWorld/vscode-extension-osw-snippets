{
	"createObject": {
		"prefix": "obj",
		"body": [
			"{",
			"\t$0",
			"}"
		],
		"description": "Create a JSON object."
	},
	"label": {
		"prefix": "label",
		"body": [
			"\"label\": [",
			"    {\"text\": \"$1\", \"lang\": \"en\"},",
			"    {\"text\": \"$2\", \"lang\": \"de\"}",
			"],"
		],
		"description": "Dual language label as used in OpenSemanticLab"
	},
	"context": {
		"prefix": "context",
		"description": "Inserts the context object. The first variable is the category of which the category defined in the jsonschema is inheritting. The second variable is a property mapping.",
		"scope": "json",
		"body": [
			"\"@context\": [\r",
			"    \"/wiki/Category:$1?action=raw&slot=jsonschema\",\r",
			"    {\r",
			"        \"$2\": \"$3\"\r",
			"    }\r",
			"],$0"
		]
	},
	"description": {
		"prefix": "desc",
		"description": "",
		"scope": "json",
		"body": [
			"\"description\": \"$1\",",
			"\"description*\": {",
			"    \"en\": \"$2\",",
			"    \"de\": \"$3\"",
			"},$0"
		]
	},
	"title": {
		"prefix": "title",
		"description": "",
		"scope": "json",
		"body": [
			"\"title\": \"$1\",",
			"\"title*\": {",
			"    \"en\": \"$2\",",
			"    \"de\": \"$3\"",
			"},$0"
		]
	},
	"uuid": {
		"prefix": "uuid",
		"description": "Inserts a UUID property as commonly used in OSW",
		"scope": "json",
		"body": [
			"\"uuid\": {",
			"    \"type\": \"string\",",
			"    \"format\": \"uuid\",",
			"    \"options\": {",
			"        \"hidden\": true",
			"    }",
			"},$0"
		]
	},
	"properties": {
		"prefix": "props",
		"description": "Inserts a properties object into a json object",
		"scope": "json",
		"body": [
			"\"properties\": {",
			"    $1",
			"},$0"
		]
	},
	"evalTemplateWikitext": {
		"prefix": "evalwiki",
		"description": "Inserts an eval_template block - enables the execution of a wikitemplate in a json property field",
		"scope": "json",
		"body": [
			"\"eval_template\": {",
			"    \"type\": \"wikitext\",",
			"    \"mode\": \"render\",",
			"    \"value\": \"{{{$1|}}}\"",
			"},$0"
		]
	},
	"defaultProperties": {
		"prefix": "def",
		"description": "Default properties - properties that are displayed in the form rendering of a json schema by default",
		"scope": "json",
		"body": [
			"\"defaultProperties\": [",
			"    \"$1\"",
			"],$0"
		]
	},
	"required": {
		"prefix": "req",
		"description": "Required properties - empty list. To be placed within a json object",
		"scope": "json",
		"body": [
			"\"required\": [",
			"    \"$1\"",
			"],$0"
		]
	},
	"allOF": {
		"prefix": "allof",
		"description": "Custom inheritance from another category",
		"scope": "json",
		"body": [
			"\"allOf\": [",
			"    {",
			"        \"\\$ref\": \"/wiki/Category:$1?action=raw&slot=jsonschema\"",
			"    }",
			"],$0"
		]
	},
	"property": {
		"prefix": "prop",
		"description": "object property",
		"scope": "json",
		"body": [
			"\"$1\": {",
			"    \"title\": \"$2\",",
			"    \"title*\" : {",
			"        \"en\": \"$3\",",
			"        \"de\": \"$4\"",
			"    },",
			"    \"description\": \"$5\",",
			"    \"description*\" : {",
			"        \"en\": \"$6\",",
			"        \"de\": \"$7\"",
			"    },",
			"    \"type\": \"$8\",    ",
			"},$0"
		]
	},
	"propertyOfTypeString": {
		"prefix": "strprop",
		"description": "object property of type string",
		"scope": "json",
		"body": [
			"\"$1\": {",
			"    \"title\": \"$2\",",
			"    \"title*\" : {",
			"        \"en\": \"$3\",",
			"        \"de\": \"$4\"",
			"    },",
			"    \"description\": \"$5\",",
			"    \"description*\" : {",
			"        \"en\": \"$6\",",
			"        \"de\": \"$7\"",
			"    },",
			"    \"type\": \"string\",",
			"    \"format\": \"$8\"",
			"},$0"
		]
	},
	"rdftype": {
		"prefix": "rdftype",
		"description": "rdftype",
		"scope": "json",
		"body": [
			"\"rdf_type\": {\r",
			"    \"title\": \"RDF type\",\r",
			"    \"title*\": {\r",
			"        \"de\": \"RDF-Typ\"\r",
			"    },\r",
			"    \"type\": \"string\",\r",
			"    \"default\": \"$1\",\r",
			"    \"options\": {\r",
			"        \"hidden\": true\r",
			"    }\r",
			"},$0"
		]
	}
}